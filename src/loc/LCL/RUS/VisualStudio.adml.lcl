        <Tgt Cat="AppData" Stat="Loc" Orig="New">
          <Val><![CDATA[Требовать утверждения гостей]]></Val>
        </Tgt>
      </Str>
      <Disp Icon="Str" />
    </Item>
    <Item ItemId="0;98e4c734144aca150bda5acb1220c97f" ItemType="42;XML:Text" PsrId="210" Leaf="true">
